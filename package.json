{
  "name": "@megafon/megafon-ui",
  "version": "1.0.11",
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MegafonWebLab/megafon-ui.git"
  },
  "author": "MegaFon",
  "license": "MIT",
  "scripts": {
    "build": "gulp build",
    "coverage": "npm run coverage:unit",
    "coverage:unit": "jest --ci --collectCoverage --coverageDirectory=coverage",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "lint": "npm run lint:ts && npm run lint:less",
    "lint:less": "stylelint --syntax less src/**/*.less src/**/**/*.less",
    "lint:ts": "tslint -c tslint.json '{src,scripts}/**/*.ts?(x)'",
    "setup": "yarn install --pure-lock",
    "prepublishOnly": "yarn setup && yarn build",
    "test": "npm run test:unit",
    "test:unit": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "babelConfig": {
          "presets": [
            "@babel/env",
            "@babel/react"
          ]
        }
      }
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/scripts/styleMock.ts",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/fileMock.ts"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/utils/modernizr.ts"
    ],
    "coverageDirectory": "coverage",
    "testURL": "http://localhost/",
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/src/utils/modernizr.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/setupEnzyme.ts"
  },
  "peerDependencies": {
    "react": ">=16.4.1",
    "react-dom": ">=16.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-block-scoping": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@svgr/cli": "^2.1.1",
    "@svgr/core": "^2.1.1",
    "@svgr/webpack": "^2.1.1",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.12",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.3.1",
    "@types/jsdom": "^11.0.6",
    "@types/react": "16.4.7",
    "@types/react-dom": "^16.0.6",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "del": "^3.0.0",
    "docz": "^0.9.2",
    "docz-plugin-css": "^0.9.2",
    "docz-theme-default": "^0.9.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "glob": "^7.1.2",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-less": "^4.0.1",
    "gulp-typescript": "^5.0.0-alpha.3",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.4.2",
    "jsdom": "^11.12.0",
    "less-plugin-autoprefix": "^2.0.0",
    "merge2": "^1.2.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.1",
    "stylelint": "^9.5.0",
    "through2": "^2.0.3",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "classnames": "^2.2.6",
    "deep-equal": "^1.0.1",
    "prop-types": "^15.6.2",
    "react-input-mask": "^2.0.3",
    "react-popper": "0.10.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
